{
  "name": "webpack-demo",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "commit": "git add . && git-cz",
    "format": "prettier --write {{changed}} \"test/**/*.js\" \"src/**/*.js\" \"src/**/*.vue\" \"src/**/*.json\" \"src/**/*.css\" \"src/**/*.scss\" ",
    "precommit": "lint-staged",
    "watch": "webpack --mode=development --watch src/develop-environment",
    "start": "webpack-dev-server --open http://localhost:8080/dist",
    "resource": "npx webpack --config webpack.config.resource.js",
    "output": "npx webpack --config webpack.config.output.js",
    "environment": "npx webpack --config webpack.config.environment.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,json,vue,css,scss,md}": [
      "prettier --write {{changed}}"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^8.2.1",
    "css-loader": "^3.5.2",
    "csv-loader": "^3.0.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
